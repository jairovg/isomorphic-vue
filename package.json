{
  "name": "isomorphic-vue",
  "version": "1.0.0",
  "description": "Mirror example from isomorphic dev js in vue",
  "scripts": {
    "build:browser": "node_modules/.bin/webpack --config setup/webpack.config.client.js --progress --hide-modules",
    "build:server": "node_modules/.bin/webpack --config setup/webpack.config.server.js --progress --hide-modules",
    "predev": "npm run build:browser && npm run build:server",
    "dev": "node server/index.js",
    "dev:debug": "node --inspect --debug-brk server/index.js",
    "start": "webpack-dev-server --config setup/webpack.config.client.js --content-base ./src --inline --hot"
  },
  "dependencies": {
    "@babel/register": "^7.10.1",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-router": "^3.3.2",
    "vue-server-renderer": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-merge": "^4.2.2"
  },
  "author": "jairovg",
  "license": "ISC"
}
