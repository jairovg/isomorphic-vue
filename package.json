{
  "name": "isomorphic-vue",
  "version": "1.0.0",
  "description": "Mirror example from isomorphic dev js in vue",
  "scripts": {
    "build:browser": "node_modules/.bin/webpack --config setup/webpack.config.client.js --progress --hide-modules",
    "build:server": "node_modules/.bin/webpack --config setup/webpack.config.server.js --progress --hide-modules",
    "predev": "npm run build:browser && npm run build:server",
    "dev": "node server/index.js",
    "dev:debug": "node --inspect --debug-brk server/index.js",
    "start": "webpack-dev-server --inline --hot"
  },
  "dependencies": {
    "babel-register": "6.26.0",
    "vue": "2.5.13",
    "vue-router": "3.0.1",
    "vue-server-renderer": "2.5.13"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "css-loader": "0.28.9",
    "eslint": "4.18.0",
    "eslint-loader": "1.9.0",
    "node-sass": "4.7.2",
    "sass-loader": "6.0.6",
    "vue-template-compiler": "2.5.13",
    "vue-loader": "14.1.1",
    "vue-style-loader": "4.0.2",
    "webpack": "3.11.0",
    "webpack-node-externals": "1.6.0",
    "webpack-merge": "4.1.1"
  },
  "author": "jairovg",
  "license": "ISC"
}
